    <tags>CSF Westwood Red-Alert-2 RA2</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="MadMilkman.Ini" version="1.0.6.0" />
      </group>
    </dependencies>

  </metadata>
</package>